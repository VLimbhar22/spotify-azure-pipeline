{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifydatafactoryved"
		},
		"AzureSpotifyconn_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSpotifyconn'"
		},
		"AzureSpotifydlconn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureSpotifydlconn'"
		},
		"AzureSpotifyconn_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyazureserver.database.windows.net"
		},
		"AzureSpotifyconn_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifyazureserver"
		},
		"AzureSpotifyconn_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminved"
		},
		"AzureSpotifydlconn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifyprojectstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifydatafactoryved_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSpotifyconn_password": {
						"value": "[parameters('AzureSpotifyconn_password')]"
					},
					"AzureSpotifydlconn_accountKey": {
						"value": "[parameters('AzureSpotifydlconn_accountKey')]"
					},
					"AzureSpotifyconn_properties_typeProperties_server": {
						"value": "[parameters('AzureSpotifyconn_properties_typeProperties_server')]"
					},
					"AzureSpotifyconn_properties_typeProperties_database": {
						"value": "[parameters('AzureSpotifyconn_properties_typeProperties_database')]"
					},
					"AzureSpotifyconn_properties_typeProperties_userName": {
						"value": "[parameters('AzureSpotifyconn_properties_typeProperties_userName')]"
					},
					"AzureSpotifydlconn_properties_typeProperties_url": {
						"value": "[parameters('AzureSpotifydlconn_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}